    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tutor_navigation"
    app:startDestination="@+id/navigation_course">

    <fragment
        android:id="@+id/navigation_course"
        android:name="com.example.beuon.roleMenu.ui.courses.CourseFragment"
        android:label="@string/title_course"
